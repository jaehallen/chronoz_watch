{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"tabWidth": 2,
	"plugins": [
		"prettier-plugin-svelte"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"tabWidth": 2,
				"useTabs": false
			}
		},
		{
			"files": "*.ts",
			"options": {
				"tabWidth": 2,
				"useTabs": false
			}
		}
	]
}